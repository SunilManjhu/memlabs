  <MinimumSeverity>Information</MinimumSeverity>
  <LogMaxSize>3</LogMaxSize>
  <LogMaxCount>5</LogMaxCount>
  <Proxy method="NoProxy" />
  <Smtp active="False">
    <Sender />
    <Recipients />
    <CCRecipients />
    <Server authMethod="Anonymous" login="" password="" port="25" tls="False"></Server>
    <NoEmailForManualSync>False</NoEmailForManualSync>
    <AdditionalEmailBodyText />
    <EmailSubject />
  </Smtp>
  <Notifications enable="False" />
  <ConfigMgrSUPSync>False</ConfigMgrSUPSync>
  <LocalContentRepository />
  <UseLocalContentRepositoryFirst>False</UseLocalContentRepositoryFirst>
  <DeleteLocalContentFile>False</DeleteLocalContentFile>
  <DisableSelfUpdate>False</DisableSelfUpdate>
  <AllowPreviewBuilds>False</AllowPreviewBuilds>
  <TemporaryFolderLocation>%Temp%</TemporaryFolderLocation>
  <DisableSccmPublishing>False</DisableSccmPublishing>
  <DisableWsusPublishing>False</DisableWsusPublishing>
  <CveFilePath />
  <ScanSccm Database="CM_TEMPLATESITECODE" AuthMethod="CurrentUser" AutoEnrollUpdates="True" UpdateThreshold="1" AutoEnrollUnderThresholdAsMetadataOnly="True" AutoEnrollApplications="True" ApplicationThreshold="1">TEMPLATESQLSERVER</ScanSccm>
  <!--  Base Install  -->
  <AllowPackageCreation SccmServer="TEMPLATESITESERVER" DistributeToAllDPs="True" DelayAppUpgrade="0" EnableAppRetention="False" AppRetentionRange="0">True</AllowPackageCreation>
  <MoveApplications ConsoleFolderPath="Applications" />
  <SignDetectionScripts>True</SignDetectionScripts>
  <PackageSourceFolder>\\TEMPLATEFILESERVER\PMPCApps</PackageSourceFolder>
  <AllowAppTaskSequence>True</AllowAppTaskSequence>
  <AllowDPFromSiteDefaultBoundary>True</AllowDPFromSiteDefaultBoundary>
  <RemoveAdminCategoriesFromRetainedApps>False</RemoveAdminCategoriesFromRetainedApps>
  <RemoveAppVersion>False</RemoveAppVersion>
  <SmsCredential Login="" Password="">False</SmsCredential>
