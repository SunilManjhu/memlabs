  <UseTrialCatalog>True</UseTrialCatalog>
  <CatalogURL>Limited Trial Mode Activated</CatalogURL>
  <ShowUpdates usesystemaccount="False" login="" password="">False</ShowUpdates>
  <RepublishedUpdatesAppendDatetime>True</RepublishedUpdatesAppendDatetime>
