{
    "globalOptions": {
        "prefix": "CB",        
        "installConfigMgr": true,
        "cmVersion": "current-branch",
        "upgradeToLatest": true,
        "pushClients": true,
        "siteCodeCS": "CS1",
        "siteCodePS": "PS1",
        "installDirCS": "<default>",
        "installDirPS": "<default>",
        "network": "192.168.1.0"
    },
    "virtualMachines": [
        {
            "vmName": "CM-DC1",
            "joinDomain": "contoso.com",
            "operatingSystem": "Server 2022",
            "role": "DC",
            "memory": "4GB",
            "virtualProcs": 4            
        },
        {
            "vmName": "CM-MP1",
            "joinDomain": "contoso.com",
            "operatingSystem": "Server 2022",
            "role": "DPMP",
            "memory": "4GB",
            "virtualProcs": 4
        },
        {
            "vmName": "CM-SITE1",
            "joinDomain": "contoso.com",
            "operatingSystem": "Server 2022",
            "role": "PS",
            "memory": "16GB",
            "virtualProcs": 8
        },
        {
            "vmName": "CM-CL1",
            "joinDomain": "contoso.com",
            "operatingSystem": "Windows 10 21H1 (64-bit)",
            "role": "DomainMember",
            "memory": "4GB",
            "virtualProcs": 4
        },
        {
            "vmName": "CM-CL2",
            "joinDomain": "contoso.com",
            "operatingSystem": "Windows 10 21H1 (64-bit)",
            "role": "DomainMember",
            "memory": "4GB",
            "virtualProcs": 4
        }
    ]
